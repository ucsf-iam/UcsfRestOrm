{
    "name": "ucsf/restorm-bundle",
    "type": "symfony-bundle",
    "description": "REST-based ORM for Symfony - Obtain objects from a REST service... just like using an ORM",
    "keywords": ["ucsf", "orm", "rest"],
    "license": "MIT",
    "authors": [
        {
            "name": "UCSF IT Identity & Access Management",
            "email": "iam-team@ucsf.edu"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/http-kernel": ">=2.6",
        "symfony/dependency-injection": ">=2.6",
        "symfony/framework-bundle": ">=2.6",
        "symfony/class-loader": ">=2.6",
        "symfony/yaml": ">=2.6"
    },
    "autoload": {
        "psr-0": { "Ucsf\\RestOrmBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Ucsf/LdapOrmBundle",
    "extra": {
    }
}
